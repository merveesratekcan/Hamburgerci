@{
    Layout = null;
}


<!DOCTYPE html>
<html lang="en">

@await Component.InvokeAsync("_LayoutHeaderPartialComponent")
<body>

    <!-- Begin page -->
    <div id="layout-wrapper">

        @await Component.InvokeAsync("_LayoutNavbarPartialComponent")

        @await Component.InvokeAsync("_LayoutSidebarPartialComponent")


        @RenderBody()






    </div>
    <!-- END layout-wrapper -->
    <!-- Overlay-->
    <div class="menu-overlay"></div>

    <footer class="footer">
        <div class="container-fluid">
            <div class="row">
                <div class="col-sm-6">
                    2024 © MelodyBurger.
                </div>
                <div class="col-sm-6">
                    <div class="text-sm-right d-none d-sm-block">
                        Design & Develop by Melody
                    </div>
                </div>
            </div>
        </div>
    </footer>


    <!-- jQuery  -->
    <script src="~/Admin/vertical/assets/js/jquery.min.js"></script>
    <script src="~/Admin/vertical/assets/js/bootstrap.bundle.min.js"></script>
    <script src="~/Admin/vertical/assets/js/metismenu.min.js"></script>
    <script src="~/Admin/vertical/assets/js/waves.js"></script>
    <script src="~/Admin/vertical/assets/js/simplebar.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9"></script>
    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/js/toastr.min.js"></script>


    <!-- App js -->
    <script src="~/Admin/vertical/assets/js/theme.js"></script>
    @await Component.InvokeAsync("Notyf");
    @await Html.PartialAsync("/Areas/Admin/Views/_notificationPartial.cshtml")
    @RenderSection("Scripts", required: false)


</body>

</html>